{
  "extends": [
    "next/core-web-vitals",
    "next/typescript",
    "standard",
    "plugin:tainwindcss/recommended",
    "prettier"
  ],

  "rules": {
    "react/no-unescaped-entities": "off",
    "@typescript-esLint/quotes": "off",
    "quotes": [0],
    "avoidEscape": 0,
    "allowTemplateLiterials": 0,
    "no-useless-escape": 0
  }
}
